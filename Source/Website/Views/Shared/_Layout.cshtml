<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<title>@ViewBag.Title - updateSystem.NET</title>
	<link href="@Url.Content("~/Content/layout.css")" rel="stylesheet" type="text/css" />
	<link href="@Url.Content("~/Content/tipsy.css")" rel="stylesheet" type="text/css" />
	@RenderSection("Styles", false)

	<script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
	<script src="@Url.Content("~/Scripts/jquery.tipsy.js")" type="text/javascript"></script>
	@RenderSection("Scripts", false)

</head>
	<body>
		<header>
			@Html.Partial("_Header")
		</header>
		<div class="body-content">
			@RenderBody()
		</div>
		<footer>
			@Html.Partial("_Footer")
		</footer>
	</body>
</html>
